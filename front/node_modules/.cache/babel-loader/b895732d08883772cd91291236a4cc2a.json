A;AACA,WAAA,WAAA,CAAA,IAAA,CAAA;AACA,QAAA,MAAA,EAAA,8CADA;AAEA,QAAA,KAAA,EAAA,KAAA,IAFA;AAGA,QAAA,QAAA,mBAAA,KAAA,KAAA,oBAAA,KAAA,GAAA;AAHA,OAAA;AAKA,WAAA,eAAA;AACA;AAhEA;AA1CA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!--===============Dialog Cliente==================-->\r\n    <v-dialog v-model=\"dialogClient\" width=\"600\">\r\n      <v-card>\r\n        <v-card-title>\r\n          Clientes\r\n          <v-btn color=\"#333\" icon text @click=\"addClient\">\r\n            <v-icon> add </v-icon>\r\n          </v-btn>\r\n        </v-card-title>\r\n\r\n        <v-divider></v-divider>\r\n\r\n        <v-container v-if=\"ableForm\">\r\n          <v-form\r\n            ref=\"form\"\r\n            v-model=\"valid\"\r\n            lazy-validation\r\n            style=\"padding: 20px\"\r\n          >\r\n            <v-text-field\r\n              v-model=\"name\"\r\n              label=\"Nome\"\r\n              required\r\n              @keyup=\"validate\"\r\n            ></v-text-field>\r\n\r\n            <v-text-field\r\n              v-model=\"carro\"\r\n              label=\"Carro\"\r\n              required\r\n              @keyup=\"validate\"\r\n            ></v-text-field>\r\n\r\n            <v-text-field\r\n              v-model=\"cpf\"\r\n              label=\"CPF\"\r\n              required\r\n              @keyup=\"validate\"\r\n            ></v-text-field>\r\n\r\n            <v-btn\r\n              :disabled=\"valid\"\r\n              color=\"success\"\r\n              class=\"mr-4\"\r\n              @click=\"addArray\"\r\n            >\r\n              Salvar\r\n            </v-btn>\r\n\r\n            <v-btn color=\"error\" @click=\"resetValidation\"> Cancelar </v-btn>\r\n          </v-form>\r\n\r\n          <v-divider></v-divider>\r\n        </v-container>\r\n\r\n        <v-card\r\n          max-width=\"100%\"\r\n          max-height=\"100%\"\r\n          class=\"mx-auto\"\r\n          style=\"margin: 0 !important\"\r\n        >\r\n          <v-list three-line>\r\n            <template v-for=\"(item, index) in itemsClient\">\r\n              <v-divider\r\n                v-if=\"item.divider\"\r\n                :key=\"index\"\r\n                :inset=\"item.inset\"\r\n              ></v-divider>\r\n\r\n              <v-list-item v-else :key=\"item.title\">\r\n                <v-list-item-avatar>\r\n                  <v-img :src=\"item.avatar\"></v-img>\r\n                </v-list-item-avatar>\r\n\r\n                <v-list-item-content>\r\n                  <v-list-item-title v-html=\"item.title\"></v-list-item-title>\r\n                  <v-list-item-subtitle\r\n                    v-html=\"item.subtitle\"\r\n                  ></v-list-item-subtitle>\r\n                </v-list-item-content>\r\n                <v-btn color=\"#333\" icon text @click=\"deleteClient(index)\">\r\n                  <v-icon> delete </v-icon>\r\n                </v-btn>\r\n              </v-list-item>\r\n            </template>\r\n          </v-list>\r\n        </v-card>\r\n\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n    <!--===============FIM Dialog Cliente==================-->\r\n    <!--===============Dialog Fornecedor===================-->\r\n    <v-dialog v-model=\"dialogForne\" width=\"600\">\r\n      <v-card>\r\n        <v-card-title>\r\n          Fornecedores\r\n          <v-btn color=\"#333\" icon text @click=\"addForne\">\r\n            <v-icon> add </v-icon>\r\n          </v-btn>\r\n        </v-card-title>\r\n        <v-divider></v-divider>\r\n\r\n        <v-card max-width=\"100%\" class=\"mx-auto\">\r\n          <v-list three-line>\r\n            <template v-for=\"(item, index) in itemsClient\">\r\n              <v-divider\r\n                v-if=\"item.divider\"\r\n                :key=\"index\"\r\n                :inset=\"item.inset\"\r\n              ></v-divider>\r\n\r\n              <v-list-item v-else :key=\"item.title\">\r\n                <v-list-item-avatar>\r\n                  <v-img :src=\"item.avatar\"></v-img>\r\n                </v-list-item-avatar>\r\n\r\n                <v-list-item-content>\r\n                  <v-list-item-title v-html=\"item.title\"></v-list-item-title>\r\n                  <v-list-item-subtitle\r\n                    v-html=\"item.subtitle\"\r\n                  ></v-list-item-subtitle>\r\n                </v-list-item-content>\r\n              </v-list-item>\r\n            </template>\r\n          </v-list>\r\n        </v-card>\r\n\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n    <!--===============FIM Dialog Fornecdor==================-->\r\n    <!--===============Dialog Carros=========================-->\r\n    <v-dialog v-model=\"dialogCar\" width=\"600\">\r\n      <v-card>\r\n        <v-card-title>\r\n          Carros\r\n          <v-btn color=\"#333\" icon @click=\"addCar\">\r\n            <v-icon> add </v-icon>\r\n          </v-btn>\r\n        </v-card-title>\r\n        <v-divider></v-divider>\r\n\r\n        <v-card max-width=\"100%\" class=\"mx-auto\">\r\n          <v-list three-line>\r\n            <template v-for=\"(item, index) in itemsClient\">\r\n              <v-divider\r\n                v-if=\"item.divider\"\r\n                :key=\"index\"\r\n                :inset=\"item.inset\"\r\n              ></v-divider>\r\n\r\n              <v-list-item v-else :key=\"item.title\">\r\n                <v-list-item-avatar>\r\n                  <v-img :src=\"item.avatar\"></v-img>\r\n                </v-list-item-avatar>\r\n\r\n                <v-list-item-content>\r\n                  <v-list-item-title v-html=\"item.title\"></v-list-item-title>\r\n                  <v-list-item-subtitle\r\n                    v-html=\"item.subtitle\"\r\n                  ></v-list-item-subtitle>\r\n                </v-list-item-content>\r\n              </v-list-item>\r\n            </template>\r\n          </v-list>\r\n        </v-card>\r\n\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n    <!--===============FIM Dialog Carros=========================-->\r\n    <!--===============Dialog Peças==============================-->\r\n    <v-dialog v-model=\"dialogParts\" width=\"600\">\r\n      <v-card>\r\n        <v-card-title>\r\n          Peças\r\n          <v-btn color=\"#333\" icon @click=\"addParts\">\r\n            <v-icon> add </v-icon>\r\n          </v-btn>\r\n        </v-card-title>\r\n        <v-divider></v-divider>\r\n\r\n        <v-card max-width=\"100%\" class=\"mx-auto\">\r\n          <v-list three-line>\r\n            <template v-for=\"(item, index) in itemsClient\">\r\n              <v-divider\r\n                v-if=\"item.divider\"\r\n                :key=\"index\"\r\n                :inset=\"item.inset\"\r\n              ></v-divider>\r\n\r\n              <v-list-item v-else :key=\"item.title\">\r\n                <v-list-item-avatar>\r\n                  <v-img :src=\"item.avatar\"></v-img>\r\n                </v-list-item-avatar>\r\n\r\n                <v-list-item-content>\r\n                  <v-list-item-title v-html=\"item.title\"></v-list-item-title>\r\n                  <v-list-item-subtitle\r\n                    v-html=\"item.subtitle\"\r\n                  ></v-list-item-subtitle>\r\n                </v-list-item-content>\r\n              </v-list-item>\r\n            </template>\r\n          </v-list>\r\n        </v-card>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n    <!--===============FIM Dialog Peças==============================-->\r\n    <v-card style=\"border-radius: 0px; height: 100%\">\r\n      <v-navigation-drawer\r\n        v-model=\"drawer\"\r\n        :mini-variant=\"true\"\r\n        color=\"#444444\"\r\n        permanent\r\n      >\r\n        <v-list-item class=\"px-2\" style=\"padding-top: 55px\">\r\n          <v-list-item-avatar>\r\n            <v-img src=\"https://randomuser.me/api/portraits/men/85.jpg\"></v-img>\r\n          </v-list-item-avatar>\r\n\r\n          <v-list-item-title>John Leider</v-list-item-title>\r\n\r\n          <v-btn icon>\r\n            <v-icon>mdi-chevron-left</v-icon>\r\n          </v-btn>\r\n        </v-list-item>\r\n        <v-list>\r\n          <v-list-item\r\n            v-for=\"(item, k) in items\"\r\n            :key=\"item.title\"\r\n            link\r\n            @click=\"sideBarButtons(k)\"\r\n          >\r\n            <v-list-item-icon>\r\n              <v-icon color=\"grey lighten-4\">\r\n                {{ item.icon }}\r\n              </v-icon>\r\n            </v-list-item-icon>\r\n\r\n            <v-list-item-content>\r\n              <v-tooltip>\r\n                <v-list-item-title>{{ item.title }}</v-list-item-title>\r\n              </v-tooltip>\r\n            </v-list-item-content>\r\n          </v-list-item>\r\n        </v-list>\r\n      </v-navigation-drawer>\r\n    </v-card>\r\n  </div>\r\n</template>\r\n<script>\r\nimport swal from \"sweetalert2\";\r\nexport default {\r\n  component: { swal },\r\n  data() {\r\n    return {\r\n      drawer: true,\r\n      items: [\r\n        { title: \"Clientes\", icon: \"mdi-account\" },\r\n        { title: \"Fornecedores\", icon: \"mdi-account-group-outline\" },\r\n        { title: \"Veiculos\", icon: \"drive_eta\" },\r\n        { title: \"Peças\", icon: \"handyman\" },\r\n      ],\r\n      mini: true,\r\n      dialogClient: false,\r\n      dialogForne: false,\r\n      dialogCar: false,\r\n      dialogParts: false,\r\n\r\n      itemsClient: [\r\n        {\r\n          avatar: \"https://cdn.vuetifyjs.com/images/lists/1.jpg\",\r\n          title: \"Felipe\",\r\n          subtitle: `Carro: Corsa, CPF: 000000.`,\r\n        },\r\n        {\r\n          avatar: \"https://cdn.vuetifyjs.com/images/lists/2.jpg\",\r\n          title: \"Matheus\",\r\n          subtitle: `Carro: Sandero, CPF: 000000.`,\r\n        },\r\n        {\r\n          avatar: \"https://cdn.vuetifyjs.com/images/lists/3.jpg\",\r\n          title: \"Fernando\",\r\n          subtitle:\r\n            'Carro: Etios, CPF: 000000.',\r\n        },\r\n      ],\r\n      valid: false,\r\n      name: \"\",\r\n      cpf: \"\",\r\n      carro: \"\",\r\n      ableForm: false,\r\n    };\r\n  },\r\n  methods: {\r\n    sideBarButtons(item) {\r\n      switch (item) {\r\n        case 0:\r\n          console.log(\"cliente\");\r\n          this.dialogClient = true;\r\n          break;\r\n        case 1:\r\n          console.log(\"Fornecedore\");\r\n          this.dialogForne = true;\r\n          break;\r\n        case 2:\r\n          console.log(\"Carros\");\r\n          this.dialogCar = true;\r\n          break;\r\n        case 3:\r\n          console.log(\"Peças\");\r\n          this.dialogParts = true;\r\n          break;\r\n      }\r\n    },\r\n    addClient() {\r\n      this.ableForm = true;\r\n    },\r\n    addForne(e) {\r\n      console.log(e);\r\n    },\r\n    addCar(e) {\r\n      console.log(e);\r\n    },\r\n    addParts(e) {\r\n      console.log(e);\r\n    },\r\n    deleteClient(index) {\r\n      swal.fire({\r\n        title: \"Tem certeza que dejesa excluir?\",\r\n        icon: \"warning\",\r\n        confirmButtonText: \"Sim\",\r\n        showCancelButton: true,\r\n        cancelButtonText: \"Não\",\r\n      }).then((resultado) => {\r\n        if (resultado.isConfirmed) {\r\n          this.itemsClient.splice(index, 1);\r\n        }\r\n      });\r\n    },\r\n    validate() {\r\n      if (this.name !== \"\" && this.cpf !== \"\" && this.carro !== \"\") {\r\n        this.valid = false;\r\n      } else {\r\n        this.valid = true;\r\n      }\r\n    },\r\n    resetValidation() {\r\n      (this.name = \"\"), (this.cpf = \"\"), (this.carro = \"\"), (this.valid = true);\r\n      this.ableForm = false;\r\n    },\r\n    addArray() {\r\n      this.itemsClient.push({\r\n        avatar: \"https://cdn.vuetifyjs.com/images/lists/1.jpg\",\r\n        title: this.name,\r\n        subtitle: `Carro: ${this.carro}, CPF: ${this.cpf}`,\r\n      });\r\n      this.resetValidation();\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.v-card__actions {\r\n  padding: 0 !important;\r\n}\r\n.swal2-popup {\r\n    font-family: 'Roboto';\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}