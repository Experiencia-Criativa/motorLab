e\"\n              label=\"Nome\"\n              required\n              @keyup=\"validate\"\n            ></v-text-field>\n\n            <v-text-field\n              v-model=\"dtNasc\"\n              label=\"Cod.Fornecedor\"\n              required\n              @keyup=\"validate\"\n            ></v-text-field>\n\n            <v-text-field\n              v-model=\"cnpj\"\n              label=\"CNPJ\"\n              required\n              @keyup=\"validate\"\n            ></v-text-field>\n\n            <v-btn\n              :disabled=\"valid\"\n              color=\"success\"\n              class=\"mr-4\"\n              @click=\"addArray(dialogSelecionado)\"\n            >\n              Salvar\n            </v-btn>\n\n            <v-btn color=\"error\" @click=\"resetValidation\"> Cancelar </v-btn>\n          </v-form>\n          <v-divider></v-divider>\n        </v-container>\n        <v-card max-width=\"100%\" class=\"mx-auto\">\n          <v-list three-line>\n            <template v-for=\"(item, index) in itemsForne\">\n              <v-divider\n                v-if=\"item.divider\"\n                :key=\"index\"\n                :inset=\"item.inset\"\n              ></v-divider>\n\n              <v-list-item v-else :key=\"item.nomeForne\">\n                <v-list-item-avatar>\n                  <v-img :src=\"item.avatar\"></v-img>\n                </v-list-item-avatar>\n\n                <v-list-item-content>\n                  <v-list-item-title\n                    v-html=\"item.nomeForne\"\n                  ></v-list-item-title>\n                  <v-list-item-subtitle\n                    v-html=\"item.subtitle\"\n                  ></v-list-item-subtitle>\n                </v-list-item-content>\n              </v-list-item>\n            </template>\n          </v-list>\n        </v-card>\n\n        <v-card-actions>\n          <v-spacer></v-spacer>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n    <!--===============FIM Dialog Fornecdor=