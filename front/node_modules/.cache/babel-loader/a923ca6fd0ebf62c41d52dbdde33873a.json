A,OAAA;AACA,OAFA;AAGA,WAAA,SAAA,GAAA,CAAA,CAAA,KAAA,CAAA,KAAA;AACA,WAAA,WAAA,GAAA,CAAA,CAAA,KAAA,CAAA,OAAA;AACA,WAAA,cAAA,CAAA,KAAA,WAAA;AACA,KArCA;AAsCA,IAAA,WAtCA,uBAsCA,CAtCA,EAsCA;AAAA;;AACA,WAAA,UAAA,GAAA,SAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,SAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,cAAA;AACA,OAJA,EAIA,IAJA,CAAA;;AAKA,UAAA,CAAA,CAAA,IAAA,KAAA,OAAA,EAAA;AACA,aAAA,eAAA,GAAA,KAAA;AACA,aAAA,iBAAA;AACA;AACA,KAnDA;AAoDA,IAAA,iBApDA,+BAoDA;AAAA;;AACA,UAAA,KAAA,GAAA,KAAA,MAAA,CAAA,SAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,IAAA,KAAA,MAAA,CAAA,iBAAA,CAAA,IAAA;AAAA,OADA,CAAA;AAGA,UAAA,OAAA,GAAA;AACA,QAAA,IAAA,EAAA,KAAA,aADA;AAEA,QAAA,KAAA,EAAA,KAAA,gBAFA;AAGA,QAAA,GAAA,EAAA,KAAA,kBAHA;AAIA,QAAA,KAAA,EAAA,KAAA,WAAA,CAAA,GAJA;AAKA,QAAA,KAAA,EAAA,CALA;AAMA,QAAA,OAAA,EAAA,KAAA,WANA;AAOA,QAAA,KAAA,EAAA,KAAA;AAPA,OAAA;AASA,WAAA,MAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA,EAAA,OAAA;AACA,KAlEA;AAmEA,IAAA,cAnEA,0BAmEA,WAnEA,EAmEA;AAAA;;AACA,UAAA,kBAAA,GAAA,KAAA,WAAA,CAAA,SAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,OAAA,KAAA,WAAA;AAAA,OAAA,CAAA,CADA,CAEA;;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,WAAA,CAAA,kBAAA,EAAA,KAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAFA;AAGA;AA1EA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <v-dialog\r\n      v-model=\"verEventoDialog\"\r\n      width=\"600\"\r\n      :persistent=\"loading\"\r\n      @click:outside=\"!loading ? salvaEdicaoEvento() : 1\"\r\n    >\r\n      <v-card :loading=\"loading\">\r\n        <v-card-title>\r\n          {{ nomeEventoTxt }}\r\n          <v-menu\r\n            v-model=\"menu\"\r\n            :close-on-content-click=\"false\"\r\n            :nudge-width=\"200\"\r\n            offset-x\r\n          >\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-icon\r\n            :color=\"colorPicker.hex == undefined ? noColor : colorPicker.hex\"\r\n            size=\"28\"\r\n            v-bind=\"attrs\"\r\n            v-on=\"on\"\r\n            >\r\n              fiber_manual_record\r\n            </v-icon>\r\n            </template>\r\n            <v-card>\r\n              <v-color-picker\r\n                v-model=\"colorPicker\"\r\n                dot-size=\"25\"\r\n                hide-inputs\r\n                hide-mode-switch\r\n                swatches-max-height=\"164\"\r\n              ></v-color-picker>\r\n            </v-card>\r\n          </v-menu>\r\n        </v-card-title>\r\n        <v-card-subtitle>\r\n          {{ dataEventoEntTxt }}\r\n        </v-card-subtitle>\r\n        <v-divider></v-divider>\r\n        <v-container>\r\n          <v-row style=\"padding: 0 25px; margin-top: 5px\">\r\n            <v-text-field\r\n              :color=\"iconeColor\"\r\n              v-model=\"nomeEventoTxt\"\r\n              label=\"Nome do evento\"\r\n              :append-icon=\"iconeTp\"\r\n              @keyup=\"editaEvento\"\r\n            >\r\n            </v-text-field>\r\n          </v-row>\r\n          <v-row style=\"padding: 0 15px\">\r\n            <v-col>\r\n              <v-text-field\r\n                :color=\"iconeColor\"\r\n                v-model=\"dataEventoEntTxt\"\r\n                label=\"Hora inicio evento\"\r\n                v-mask=\"['####-##-## ##:##']\"\r\n                :append-icon=\"iconeTp\"\r\n                @keyup=\"editaEvento\"\r\n              >\r\n              </v-text-field>\r\n            </v-col>\r\n            <v-col>\r\n              <v-text-field\r\n                :color=\"iconeColor\"\r\n                v-model=\"dataEventoSaidaTxt\"\r\n                label=\"Hora final evento\"\r\n                v-mask=\"['####-##-## ##:##']\"\r\n                :append-icon=\"iconeTp\"\r\n                @keyup=\"editaEvento\"\r\n              >\r\n              </v-text-field>\r\n            </v-col>\r\n          </v-row>\r\n          <v-row style=\"padding: 0 15px\">\r\n            <v-col>\r\n              <v-select\r\n                v-model=\"formCliente\"\r\n                label=\"Cliente\"\r\n                :color=\"iconeColor\"\r\n                :append-icon=\"iconeTp\"\r\n                :items=\"cliente\"\r\n                @change=\"veiculoCliente(formCliente)\"\r\n                @keyup=\"editaEvento\"\r\n              >\r\n              </v-select>\r\n            </v-col>\r\n            <v-col>\r\n              <v-select\r\n                v-model=\"formCarro\"\r\n                label=\"Carro\"\r\n                :color=\"iconeColor\"\r\n                :append-icon=\"iconeTp\"\r\n                :items=\"carro\"\r\n                @keyup=\"editaEvento\"\r\n              >\r\n              </v-select>\r\n            </v-col>\r\n          </v-row>\r\n        </v-container>\r\n      </v-card>\r\n    </v-dialog>\r\n    <v-sheet tile height=\"54\" class=\"d-flex\">\r\n      <v-btn icon class=\"ma-2\" @click=\"$refs.calendar.prev()\">\r\n        <v-icon>mdi-chevron-left</v-icon>\r\n      </v-btn>\r\n      <v-btn outlined class=\"ma-2\" color=\"grey darken-2\" @click=\"setToday\">\r\n        Hoje\r\n      </v-btn>\r\n      <v-spacer></v-spacer>\r\n      <v-toolbar-title style=\"margin-top: 12px\" v-if=\"$refs.calendar\">\r\n        {{ $refs.calendar.title }}\r\n      </v-toolbar-title>\r\n      <v-btn icon class=\"ma-2\" @click=\"$refs.calendar.next()\">\r\n        <v-icon>mdi-chevron-right</v-icon>\r\n      </v-btn>\r\n    </v-sheet>\r\n    <v-sheet height=\"750\">\r\n      <v-calendar\r\n        ref=\"calendar\"\r\n        v-model=\"focus\"\r\n        :weekdays=\"weekday\"\r\n        :first-interval=\"1\"\r\n        :interval-count=\"24\"\r\n        :type=\"type\"\r\n        :events=\"events\"\r\n        :event-overlap-mode=\"mode\"\r\n        :event-overlap-threshold=\"60\"\r\n        @click:date=\"type === 'month' ? viewDay($event) : (type = 'month')\"\r\n        @click:event=\"verEvento\"\r\n      ></v-calendar>\r\n    </v-sheet>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport moment from \"moment\";\r\nimport { mask } from \"vue-the-mask\";\r\nimport indexDb from \"../indexedDB/indexdb\"\r\nexport default {\r\n  directives: { mask },\r\n  data: () => ({\r\n    focus: \"\",\r\n    type: \"month\",\r\n    types: [\"month\", \"week\", \"day\"],\r\n    mode: \"stack\",\r\n    modes: [\"stack\", \"column\"],\r\n    weekday: [0, 1, 2, 3, 4, 5, 6],\r\n    weekdays: [\r\n      { text: \"Sun - Sat\", value: [0, 1, 2, 3, 4, 5, 6] },\r\n      { text: \"Mon - Sun\", value: [1, 2, 3, 4, 5, 6, 0] },\r\n      { text: \"Mon - Fri\", value: [1, 2, 3, 4, 5] },\r\n      { text: \"Mon, Wed, Fri\", value: [1, 3, 5] },\r\n    ],\r\n    events: [],\r\n    eventoSelecionado: [],\r\n    verEventoDialog: false,\r\n    nomeEventoTxt: \"\",\r\n    dataEventoEntTxt: \"\",\r\n    dataEventoSaidaTxt: \"\",\r\n    iconeTp: \"\",\r\n    iconeColor: \"#7d7d7d\",\r\n    loading: false,\r\n    itemsClient: {},\r\n    formCliente: '',\r\n    formCarro: '',\r\n    carro: [],\r\n    cliente: [],\r\n    colorPicker: {},\r\n    menu: false,\r\n    noColor: '#FF0000',\r\n  }),\r\n  beforeMount() {\r\n    this.getEvents();\r\n    indexDb.getDataBase('clientes').then(clientes => {\r\n      this.itemsClient = clientes\r\n    })\r\n\r\n  },\r\n  methods: {\r\n    getEvents() {\r\n      this.events.push({\r\n        name: \"Raggi\",\r\n        start: moment().format(\"YYYY-MM-DD HH:MM\"),\r\n        end: moment().format(\"YYYY-MM-DD HH:MM\"),\r\n        color: \"#ff2976\",\r\n        timed: 1,\r\n        cliente: 'Raggi',\r\n        carro: 'Sandero',\r\n      });\r\n    },\r\n    rnd(a, b) {\r\n      return Math.floor((b - a + 1) * Math.random()) + a;\r\n    },\r\n    viewDay({ date }) {\r\n      this.focus = date;\r\n      this.type = \"day\";\r\n    },\r\n    setToday() {\r\n      this.focus = \"\";\r\n    },\r\n    verEvento(e) {\r\n      this.iconeColor = \"#7d7d7d\";\r\n      this.iconeTp = \"\";\r\n      this.colorPicker.hex = e.event.color\r\n      this.eventoSelecionado = e.event;\r\n      this.nomeEventoTxt = e.event.name;\r\n      this.dataEventoEntTxt = e.event.start;\r\n      this.dataEventoSaidaTxt = e.event.end;\r\n      this.verEventoDialog = true;\r\n      this.itemsClient.forEach((element) => {\r\n      this.cliente.push(element.cliente);\r\n      });\r\n      this.formCarro = e.event.carro\r\n      this.formCliente = e.event.cliente\r\n      this.veiculoCliente(this.formCliente)\r\n    },\r\n    editaEvento(e) {\r\n      this.iconeColor = \"#7d7d7d\";\r\n      this.iconeTp = \"\";\r\n      this.loading = true;\r\n      setTimeout(() => {\r\n        this.loading = false;\r\n        this.iconeColor = \"#35f073\";\r\n        this.iconeTp = \"check_circle\";\r\n      }, 3000);\r\n      if (e.code === \"Enter\") {\r\n        this.verEventoDialog = false;\r\n        this.salvaEdicaoEvento();\r\n      }\r\n    },\r\n    salvaEdicaoEvento() {\r\n      let index = this.events.findIndex(\r\n        (f) => f.name === this.eventoSelecionado.name\r\n      );\r\n      let eventos = {\r\n        name: this.nomeEventoTxt,\r\n        start: this.dataEventoEntTxt,\r\n        end: this.dataEventoSaidaTxt,\r\n        color: this.colorPicker.hex,\r\n        timed: 1,\r\n        cliente: this.formCliente,\r\n        carro: this.formCarro,\r\n      };\r\n      this.events.splice(index, 1, eventos);\r\n    },\r\n    veiculoCliente(nomeCliente) {\r\n      let clienteSelecionado = this.itemsClient.findIndex(f => f.cliente === nomeCliente)\r\n      // this.formCarro = this.itemsClient[clienteSelecionado].carro[0]\r\n      this.carro = []\r\n      this.itemsClient[clienteSelecionado].carro.forEach((element) => {\r\n        this.carro.push(element);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.row + .row {\r\n  margin-top: 0 !important ;\r\n}\r\n.theme--dark.v-calendar-weekly .v-calendar-weekly__day {\r\n  border-right: #404040 1px solid !important;\r\n  border-bottom: #404040 1px solid !important;\r\n  color: #FFFFFF;\r\n}\r\n.theme--dark.v-calendar-weekly .v-calendar-weekly__head-weekday {\r\n    border-right: #404040 1px solid !important;\r\n    color: #FFFFFF !important;\r\n}\r\n.theme--dark.v-calendar-weekly {\r\n  background-color: #303030 !important;\r\n  border-top: #404040 1px solid !important;\r\n  border-left: #9e9e9e 1px solid !important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}